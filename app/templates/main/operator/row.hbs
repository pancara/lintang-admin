<td>{{index}}.</td>
<td>
    <button class="btn btn-xs" {{action "toggleDetail"}}>
      {{#unless detailVisible }}
        {{fa-icon "plus" }}
      {{/unless}}
      {{#if detailVisible }}
        {{fa-icon "minus" }}
      {{/if}}

    </button>
</td>
<td>
  {{#if (eq data.status 'ACTIVE')}}
      <span class="pw-label pw-label-success pw-label-xs">A</span>
  {{else}}
      <span class="pw-label pw-label-danger pw-label-xs">I&nbsp;</span>
  {{/if}}

</td>
<td>{{data.code}}</td>
<td>{{data.name}}</td>
<td>{{data.contactName}}</td>
<td>{{data.fee}}</td>
<td>
    <button class="btn btn-primary btn-xs" {{action "click"  "changeFee" data}}>
        {{fa-icon "pencil"}}
        Fee
    </button>

    <button class="btn btn-primary btn-xs" {{action "click"  "changeStatus" data}}>
        {{fa-icon "pencil"}}
        Status
    </button>
</td>